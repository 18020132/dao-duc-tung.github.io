<!DOCTYPE html>
<html lang="en-US">
  <%- include('./partials/header') %>
  <body>
    <%- include('./partials/navigation') %>

    <section class="main-content">
      <% site.posts.forEach((category) => { if (category.type === 'category' && category.title ===
      page.title) { %>
      <h1><%= category.title %></h1>
      <ul>
        <% category.childs.forEach((categoryChild) => { %>
        <li>
          <a href="<%= site.basePath %>/<%= categoryChild.url %>"><%= categoryChild.title %></a>
        </li>
        <% }) %>
      </ul>
      <% }}) %>
    </section>
    <%- include('./partials/footer') %>
  </body>
</html>
